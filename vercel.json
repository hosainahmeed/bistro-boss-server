{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/menu",
        "dest": "/index.js",
        "methods": ["GET"]
      },
      {
        "src": "/(.*)",
        "dest": "/index.js",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
      }
    ]
  }
  